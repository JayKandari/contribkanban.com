<?xml version="1.0"?>
<ruleset name="Centarro CS">
  <description>Drupal 8 coding standards</description>
  <exclude-pattern>*/vendor/*</exclude-pattern>
  <rule ref="Drupal"/>
  <rule ref="Drupal.WhiteSpace.OpenTagNewline.BlankLine">
    <severity>0</severity>
  </rule>
  <rule ref="Drupal.Commenting.ClassComment.Missing">
    <severity>0</severity>
  </rule>
    <rule ref="Drupal.Commenting.FileComment.Missing">
      <severity>0</severity>
    </rule>
</ruleset>
